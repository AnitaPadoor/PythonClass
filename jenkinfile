pipeline {
  agent any
  stages {
    
    stage('Check') {
      steps {
        bat  script: 'python -m pip install pylint'
        bat  script: 'python -m pylint calc.py'
        
      }
    }
    stage('Test') {
      steps {
        bat  script: 'python -m pip install pytest'
        bat  script: 'python -m pytest test_calc.py'
      }
    }
  }
}
